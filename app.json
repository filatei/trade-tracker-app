{
  "expo": {
    "scheme": "money-app",
    "userInterfaceStyle": "dark",
    "orientation": "default",
    "web": {
      "output": "static"
    },
    "icon": "./assets/images/icon.png",
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "statusBar": {
              "backgroundColor": "#000000",
              "barStyle": "light-content"
            }
          },
          "ios": {
            "userInterfaceStyle": "dark"
          }
        }
      ],
      "expo-secure-store",
      "expo-web-browser",
      "expo-router"
    ],
    "name": "money-app",
    "slug": "money-app",
    "updates": {
      "enabled": false,
      "fallbackToCacheTimeout": 0,
      "checkAutomatically": "NEVER"
    },
    "android": {
      "package": "ng.torama.money",
      "edgeToEdge": true
    },
    "ios": {
      "bundleIdentifier": "ng.torama.money",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "Allow access to photos"
      }
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "1104a0ab-ef2d-47c1-aea9-8d0b49d0c180"
      }
    },
    "owner": "filatei"
  }
}
